<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false" android:state_focused="true">
        <inset android:drawable="@drawable/radio_unchecked" android:insetRight="@dimen/margin_16" />
    </item>
    <item android:state_checked="false" android:state_focused="false">
        <inset android:drawable="@drawable/radio_unchecked" android:insetRight="@dimen/margin_16" />
    </item>
    <item android:state_checked="true" android:state_focused="true">
        <inset android:drawable="@drawable/radio_checked" android:insetRight="@dimen/margin_16" />
    </item>
    <item android:state_checked="true" android:state_focused="false">
        <inset android:drawable="@drawable/radio_checked" android:insetRight="@dimen/margin_16" />
    </item>
</selector>