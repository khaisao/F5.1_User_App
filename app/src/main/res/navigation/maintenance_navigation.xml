<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/maintenance_navigation"
    app:startDestination="@id/maintenanceFragment">

    <fragment
        android:id="@+id/maintenanceFragment"
        android:name="jp.careapp.counseling.android.ui.maintenance.MaintenanceFragment"
        android:label="MaintenanceFragment" >
        <action
            android:id="@+id/action_maintenanceFragment_to_webViewFragment2"
            app:enterAnim="@anim/slide_up"
            app:popExitAnim="@anim/slide_down"
            app:destination="@id/webViewFragment2" />
        <argument
            android:name="type"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/webViewFragment2"
        android:name="jp.careapp.counseling.android.ui.webView.WebViewFragment"
        android:label="WebViewFragment" />
</navigation>